#include <stdio.h>
#include <stdlib.h>

int main()
{
int n[10], n2[10], i, j;
system("chcp 65001");
system("cls");
printf("Введите 10 элементов массива\n");
for (i=0;i<10;i++)
{
scanf("%d", &n[i]);
}
j=9;
for (i=0;i<10;i++)
{
n2[i]=n[j];
j--;
}
for (i=0;i<10;i++)
{
n[i]=n2[i];
printf("%d, ",n[i]);
}
return 0;
}
